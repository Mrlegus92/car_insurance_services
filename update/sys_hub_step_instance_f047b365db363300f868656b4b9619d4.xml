var key = gs.getProperty('x_360894_cis.googleAPIKey');
  
 try { 
 var r = new sn_ws.RESTMessageV2('x_360894_cis.Reverse Geocoding', 'Default GET');
    r.setQueryParameter("latlng",latlng);
